		},
		{
			"type": "entity",
			"partition": "hokkaido",
			"path": "[assembly:/_pro/scenes/missions/hokkaido/gamechanger_hokkaido_minefieldvipsuites.brick].entitytype"
		},
		{
			"type": "entity",
			"partition": "hokkaido",
			"path": "[assembly:/_pro/scenes/missions/hokkaido/gamechanger_hokkaido_minefieldresort.brick].entitytype"
		},
		{
